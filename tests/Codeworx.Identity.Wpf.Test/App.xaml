<Application x:Class="Codeworx.Identity.Wpf.Test.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:Codeworx.Identity.Wpf.Test.ViewModel"
             xmlns:v="clr-namespace:Codeworx.Identity.Wpf.Test.View">
    <Application.Resources>
        <DataTemplate DataType="{x:Type vm:LoginViewModel}">
            <v:LoginView />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:HelloViewModel}">
            <v:HelloView />
        </DataTemplate>
    </Application.Resources>
</Application>
